{
        "configurations": [
            {
                "name": "Linux",
                "includePath": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/../linux/include",
                    "${workspaceFolder}/../linux/arch/arm/include/generated",
                    "${workspaceFolder}/../linux/arch/arm/include/uapi",
                    "${workspaceFolder}/../linux/arch/arm/include",
                    "${workspaceFolder}/../linux/arch/arm/include/generated/uapi",
                    "${workspaceFolder}/../linux/include/uapi",
                    "${workspaceFolder}/../linux/include/generated/uapi"
                ],
                "forcedInclude": [
                    "${workspaceFolder}/../linux/include/linux/compiler_types.h",
                    "${workspaceFolder}/../linux/include/linux/kconfig.h"
                ],
                "defines": [
                    "__KERNEL__",
                    "__LINUX_ARM_ARCH__=7",
                    "KBUILD_BASENAME='fpga-loader'",
                    "KBUILD_MODNAME='fpga-loader'",
                    "MODULE"],
                "compilerPath": "/usr/bin/arm-linux-gnueabihf-gcc",
                "compilerArgs": [
                    "-nostdinc"
                ],
                "cStandard": "c11",
                "intelliSenseMode": "${default}"
            }
        ],
        "version": 4
    }